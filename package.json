{
  "name": "auth-component",
  "version": "0.0.1",
  "description": "A React High Order Component to manage authentication using Redux",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive --require test/init.js test/**/*-test.js",
    "build": "mkdir -p lib && babel ./src/inex.js --out-file ./lib/index.js",
    "lint": "eslint src test",
    "prepublish": "rm -rf lib && npm run build",
    "test:cov": "babel-node $(npm bin)/babel-istanbul cover $(npm bin)/_mocha -- --require test/init.js test/**/*-test.js",
    "test:watch": "mocha --compilers js:babel-core/register --recursive --require test/init.js -w test/**/*-test.js"
  },
  "keywords": [
    "HOC",
    "High",
    "Order",
    "Component",
    "Auth",
    "React",
    "Redux"
  ],
  "author": "Matias Hernandez <matiasfh@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "^6.0.0",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-react-props": "0.0.3",
    "mocha": "^2.4.5",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "lodash-fp": "^0.10.4"
  }
}
